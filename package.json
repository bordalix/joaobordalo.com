{
  "name": "joaobordalo",
  "private": true,
  "scripts": {
    "lint": "eslint .",
    "start": "meteor run"
  },
  "dependencies": {
    "babel-runtime": "^6.20.0",
    "meteor-node-stubs": "~0.2.0"
  },
  "devDependencies": {
    "@meteorjs/eslint-config-meteor": "^1.0.5",
    "babel-eslint": "^7.2.3",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-import-resolver-meteor": "^0.4.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-meteor": "^4.0.1",
    "eslint-plugin-react": "^7.1.0"
  },
  "eslintConfig": {
    "extends": "@meteorjs/eslint-config-meteor",
    "rules": {
      "comma-dangle": "off",
      "no-multi-spaces": "off",
      "key-spacing": "off",
      "meteor/no-session": "off"
    },
    "globals": {
      "Posts": true,
      "Meteor": true,
      "Template": true,
      "Router": false,
      "Mongo": false,
      "Tracker": false,
      "Session": false,
      "RssFeed": false,
      "DocHead": false,
      "ReactiveVar": false,
      "check": false,
      "sitemaps": false,
      "moment": false,
      "camURLsForIDs": true,
      "truncateText": true,
      "analytics": false,
      "$": false,
      "makeYoutubeResponsive": true,
      "lazyLoadImages": true
    }
  }
}
